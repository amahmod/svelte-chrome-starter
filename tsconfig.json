{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "exclude": [
    "node_modules"
  ],
  "include": [
    ".plasmo/index.d.ts",
    ".plasmo/**/*",
    "./**/*.ts",
    "./**/*.tsx",
    "./**/*.svelte"
  ],
  "compilerOptions": {
    "strict": true,
    "paths": {
      "~*": [
        "./src/*"
      ]
    },
    "baseUrl": ".",
    "lib": [
      "DOM",
      "ES2017",
      "WebWorker"
    ],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "importsNotUsedAsValues": "remove"
  }
}
